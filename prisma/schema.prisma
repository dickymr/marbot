generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscriber {
  id           String   @id
  name         String
  notification  Boolean  @default(true)
  reminder     Int      @default(0)
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model Log {
  id           String      @id @default(cuid())
  source       String
  status       String
  value        String
  createdAt    DateTime    @default(now())
  updatedAt    DateTime    @updatedAt
}
